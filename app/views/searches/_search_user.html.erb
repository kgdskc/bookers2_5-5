<table  class='table'>
    <thead>
        <tr>
            <th>ユーザー名</th>
            <th colspan="3"></th>
        </tr>
    </thead>
    <tbody>
        <% users.each do |user| %>
          <tr>
            <td>
                <%= attachment_image_tag(user, :profile_image, :fill, 50, 50, fallback: "no-image-icon.jpg") %>
                <%= user.name %>
            </td>
            <td></td>
            <td><%= link_to "Show", user_path(user) %></td>
          </tr>
        <% end %>
    </tbody>
</table>